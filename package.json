{
  "name": "vue-starter",
  "version": "0.0.1",
  "description": "Starter Project for Vue with Typescript, Babel and Webpack",
  "main": "src/main.ts",
  "scripts": {
    "start": "npm run build-dev && webpack-dev-server --env.development --open",
    "bundle-dev": "npm run build-dev && webpack --env.development",
    "bundle": "npm run build && webpack",
    "build-dev": "npm run type-check && npm run build-types && babel src --out-dir lib --extensions \".ts\" --source-maps both",
    "build": "npm run type-check && babel src --out-dir lib --extensions \".ts\" --delete-dir-on-start",
    "build-types": "tsc --emitDeclarationOnly",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "karma start --single-run",
    "pretest": "npm run build-dev && babel test/unit/specs --out-dir test/lib/specs --extensions \".ts\"",
    "test:watch": "karma start",
    "pretest:watch": "npm run build-dev && babel test/unit/specs --out-dir test/lib/specs --extensions \".ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mcafee123/vue-starter.git"
  },
  "keywords": [
    "vue",
    "typescript",
    "babel",
    "webpack",
    "scss"
  ],
  "author": "Martin Affolter <martin@affolter.net>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mcafee123/vue-starter/issues"
  },
  "homepage": "https://github.com/Mcafee123/vue-starter#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.7",
    "@types/webpack": "^4.4.27",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "css-loader": "^2.1.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^4.1.0",
    "karma-babel-preprocessor": "^8.0.0-beta.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "3.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "mocha": "^6.1.4",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.2.3",
    "typescript": "^3.4.5",
    "vue": "^2.6.10",
    "vue-devtools": "^5.0.0-beta.1",
    "vue-property-decorator": "^8.1.0",
    "vue-template-compiler": "^2.6.10",
    "vue-template-loader": "^1.0.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "browserslist": "> 0.25%, not dead"
}
