{
  "name": "vue-starter",
  "version": "0.0.1",
  "description": "Starter Project for Vue with Typescript, Babel and Webpack",
  "main": "src/main.ts",
  "scripts": {
    "start": "npm run bundle && webpack-dev-server --mode development --open",
    "bundle": "npm run build && webpack",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mcafee123/vue-starter.git"
  },
  "keywords": [
    "vue",
    "typescript",
    "babel",
    "webpack",
    "scss"
  ],
  "author": "Martin Affolter <martin@affolter.net>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mcafee123/vue-starter/issues"
  },
  "homepage": "https://github.com/Mcafee123/vue-starter#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@types/node": "^11.13.5",
    "@types/webpack": "^4.4.27",
    "babel-loader": "^8.0.5",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "typescript": "^3.4.4",
    "vue": "^2.6.10",
    "vue-property-decorator": "^8.1.0",
    "vue-template-compiler": "^2.6.10",
    "vue-template-loader": "^1.0.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "browserslist": "> 0.25%, not dead"
}
